[sub_resource type="GDScript" id="GDScript_u54rr"]
script/source = "##I'm not sure if this was made on purpose or accidentally,
##but it seems to be a copy of Item(which has since been renamed
##to ItemResource). It was shadowing the class name, so I changed
##it to ObjectResource to eliminate the conflict.
class_name ObjectResource extends Resource
static var next_id:int = 0;
var id;
@export var name:String;
@export var description:String;
@export var image:Texture2D;
@export var consumable:bool;
#The most items of this type which can be contained in
#one inventory slot
@export var max_in_inv_slot:int = 1;

func _init() -> void:
	id = next_id;
	next_id += 1;
"

