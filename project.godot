; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Project Scream Jam"
config/features=PackedStringArray("4.5", "GL Compatibility")
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://Resources/default_bus_layout.tres"

[autoload]

SceneTransition="*res://Scenes/scene_transition.tscn"

[file_customization]

folder_colors={
"res://Assets/": "red",
"res://Autoloads/": "orange",
"res://Resources/": "yellow",
"res://Scenes/": "green",
"res://Scripts/": "blue"
}

[global_group]

Player="Player"
TransitionArea="Area that will teleport either Player or Killer to a new location"

[layer_names]

2d_physics/layer_1="Player"
2d_physics/layer_2="Killer"
2d_physics/layer_3="Object"
2d_physics/layer_4="Interactive"
2d_physics/layer_5="Floor"
2d_physics/layer_6="Wall"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
